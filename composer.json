{
  "name": "beastbytes/vcard",
  "type": "library",
  "description": "Create and parse vCards",
  "keywords": [
    "vCard",
    "RFC 6350"
  ],
  "license": "BSD-3-Clause",
  "authors": [{
    "name": "Chris Yates",
    "email": "chris.l.yates@gmail.com"
  }],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "roave/infection-static-analysis-plugin": "^1.0",
    "roave/security-advisories": "dev-latest",
    "vimeo/psalm": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "BeastBytes\\Vcard\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BeastBytes\\Vcard\\Tests\\": "tests"
    }
  },
  "scripts": {
    "infection": "roave-infection-static-analysis-plugin",
    "psalm": "psalm",
    "robo": "robo",
    "test": "phpunit --testdox"
  }
}
